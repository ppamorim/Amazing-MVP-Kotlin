<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amazingmvpkotlin">

  <uses-permission android:name="android.permission.INTERNET">true</uses-permission>
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">true</uses-permission>

  <application android:allowBackup="true"
      android:label="@string/app_name"
      android:icon="@mipmap/ic_launcher"
      android:theme="@style/AppTheme.Base"
      android:name=".AmazingMvpApplication">

    <activity
        android:name=".ui.activity.HomeActivity"
        android:label="@string/app_name" >
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

  </application>

</manifest>
